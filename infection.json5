{
    "source": {
        "directories": [
            "src/SprykerSdk"
        ]
    },
    "timeout": 20,
    "logs": {
        "text": "tests/_output/infection.log",
        "html": "tests/_output/infection.html",
        "summary": "tests/_output/summary.log",
        "json": "tests/_output/infection-log.json",
        "perMutator": "tests/_output/per-mutator.md"
    },
    "tmpDir": "./tests/_data/mutations",
    "mutators": {
        "global-ignore": [
            "FooClass::__construct"
        ],
        "global-ignoreSourceCodeByRegex": [
            "Assert::.*",
            "getcwd",
            "PHP_SAPI",
        ],
        "@default": true,
        "@function_signature": false,
        "CastString": {
            "ignore": [
                "SprykerSdk\\SyncApi\\SyncApiConfig::getProjectRootPath::35",
                "SprykerSdk\\SyncApi\\SyncApiConfig::getSprykRunExecutablePath::50",
                "SprykerSdk\\SyncApi\\Message\\SyncApiInfo::format",
                "SprykerSdk\\SyncApi\\Message\\SyncApiError::format",
            ],
        },
    },
    "testFramework":"codeception"
}
