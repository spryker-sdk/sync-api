<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
  <transfer name="AppCollectionApiResponse">
    <property name="AppApiResponses" type="AppApiResponse[]" singular="appApiResponse"/>
  </transfer>
  <transfer name="ApiErrorMessage">
    <property name="code" type="string"/>
    <property name="detail" type="string"/>
    <property name="status" type="integer"/>
  </transfer>
  <transfer name="AppRegistrationApiRequest">
    <property name="api" type="string"/>
    <property name="manifest" type="string"/>
    <property name="configuration" type="string"/>
    <property name="translation" type="string"/>
  </transfer>
  <transfer name="AppRegistrationApiResponse">
    <property name="connectionStatus" type="string"/>
  </transfer>
  <transfer name="AppApiResponse">
    <property name="id" type="string"/>
    <property name="name" type="string"/>
    <property name="description" type="string"/>
    <property name="status" type="string"/>
    <property name="categories" type="array[]" singular="string"/>
    <property name="labels" type="array[]" singular="string"/>
    <property name="iconUrl" type="string"/>
    <property name="manifest" type="string"/>
    <property name="configuration" type="string"/>
  </transfer>
  <transfer name="ApiErrorMessages">
    <property name="ApiErrorMessages" type="ApiErrorMessage[]" singular="apiErrorMessage"/>
  </transfer>
  <transfer name="AppConnectionApiResponse">
    <property name="id" type="string"/>
    <property name="name" type="string"/>
    <property name="description" type="string"/>
    <property name="status" type="string"/>
    <property name="categories" type="array[]" singular="string"/>
    <property name="labels" type="array[]" singular="string"/>
    <property name="iconUrl" type="string"/>
    <property name="manifest" type="string"/>
    <property name="configuration" type="string"/>
  </transfer>
  <transfer name="AppConfigurationApiResponse">
    <property name="configuration" type="string"/>
  </transfer>
  <transfer name="AppConfigurationApiRequest">
    <property name="configuration" type="string"/>
  </transfer>
</transfers>
